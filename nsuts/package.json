{
  "name": "nsuts",
  "displayName": "nsuts",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nsuts.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "NSUTS",
      "type": "object",
      "properties": {
        "nsuts.email": {
          "type": "string"
        },
        "nsuts.password": {
          "type": "string"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "codegen": "openapi-typescript ../NSUTS_fresh_API.yaml -o src/api/api.d.ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  }
}
